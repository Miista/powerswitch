{
  "manifest_version": 2,
  "name": "PowerSwitch",
  "version": "2011.9.21",
  "description": "Switch tabs by typing",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["MochiKit.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],

  "permissions": [
    "tabs",
    "notifications"
  ]
}
