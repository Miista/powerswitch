{
  "manifest_version": 2,
  "name": "PowerSwitch",
  "version": "2013.9.21",
  "description": "Switch tabs by typing",
  "background": {
    "scripts": ["MochiKit.js", "background.js"]
  },
  "commands": {
    "switch_tab": {
      "suggested_key": {
        "default": "Ctrl+K"
      },
      "description": "Bring up the tab switcher"
    }
  },
  "permissions": [
    "tabs"
  ]
}
