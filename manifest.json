{
  "name": "PowerSwitch",
  "version": "2011.1.13",
  "description": "Switch tabs by typing",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],

  "permissions": [
    "tabs",
    "notifications"
  ]
}
